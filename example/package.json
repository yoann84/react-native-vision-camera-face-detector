{
  "name": "face-detector-example",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "pods": "pod-install --quiet",
    "lint": "bun test && eslint --quiet --fix --ext .js,.ts,.tsx,.jsx .",
    "test": "tsc",
    "prebuild": "npx expo prebuild",
    "prebuild:clean": "npx expo prebuild --clean",
    "android": "bun prebuild && npx expo run:android -d",
    "android:prod": "bun prebuild && npx expo run:android -d --variant release",
    "ios": "bun prebuild && npx expo run:ios -d",
    "ios:prod": "bun prebuild && npx expo run:ios -d --configuration Release",
    "start": "expo start --dev-client"
  },
  "main": "index.js",
  "dependencies": {
    "@react-native-community/hooks": "^100.1.0",
    "@react-navigation/native": "^7.1.6",
    "@shopify/react-native-skia": "v2.0.0-next.4",
    "expo": "^53.0.19",
    "expo-application": "~6.1.5",
    "expo-build-properties": "~0.14.8",
    "expo-dev-client": "~5.2.4",
    "expo-image-picker": "~16.1.4",
    "react": "19.0.0",
    "react-native": "0.79.6",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-vision-camera": "https://github.com/yoann84/react-native-vision-camera",
    "react-native-vision-camera-face-detector": "link:../",
    "react-native-worklets-core": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@types/react": "~19.0.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.1",
    "eslint-config-expo": "~9.2.0",
    "pod-install": "^0.3.7",
    "typescript": "~5.8.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-vision-camera",
          "react-native-vision-camera-face-detector"
        ]
      }
    }
  }
}